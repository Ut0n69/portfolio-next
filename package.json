  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:ci": "yarn lint && yarn type-check",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "add": "^2.0.6",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "next": "^10.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-three-fiber": "^5.3.10",
    "react-tilt": "^0.1.4",
    "react-ztext": "^1.0.1",
    "three": "^0.124.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  }
}
